# Ignore vendor folder
/vendor
# Ignore wp-cli command
/wp-cli.phar
# Ignore autogenerated WordPress files and folder
/public/.htaccess
/public/wp
/public/content/*
!/public/content/uploads
# Ignore autogenerated Elementor files and folder
/public/content/uploads/elementor/css
/public/content/uploads/elementor/cache
/public/content/uploads/elementor/google-fonts
# Exclude languages folder
!/public/content/languages
# Ignore wp-optimize log files
/public/content/uploads/wpo/logs/*.log
# Exclude Pro plugin from gitignore
!/public/content/plugins
/public/content/plugins/*
!/public/content/plugins/advanced-custom-fields-pro
!/public/content/plugins/elementor
!/public/content/plugins/elementor-pro
!/public/content/plugins/revslider
# Exclude theme
!/public/content/themes
/public/content/themes/*
!/public/content/themes/codevelopers
