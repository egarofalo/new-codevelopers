{
    "publicPath": "dist",
    "browserSync": {
        "proxy": "https://wpelementor.test",
        "files": ["**/*.(php)", "**/*.(js|css)"],
        "open": false
    },
    "extractVendorLibraries": [],
    "entries": [
        {
            "output": "main.js",
            "input": "main.js"
